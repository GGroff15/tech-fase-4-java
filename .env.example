# .env.example
# Required environment variables based on src/main/resources/application.yaml
# Copy this file to .env and fill in real values before running the app.

# Server port (optional, default 8080)
PORT=8080

# API key used to authorize requests to triage endpoints
TRIAGE_API_KEY=your-secret-api-key-here

# Gemini (LLM) provider API key
GEMINI_API_KEY=your-gemini-api-key-here

# Optional: override Gemini model (if you want to change it)
# GEMINI_MODEL=gemini-2.5-flash
